@model IEnumerable<WebMinhaFinancas.Models.Entitty.TypeFixedBill>
@{
    ViewData["Title"] = "Tipo de Conta Fixa";
}
<h2 class="jumbotron">@ViewData["Title"]</h2>

<table class="table table-striped table-hover table-responsive">
    <thead>
        <tr>
            <th>
                Nome
            </th>
            <th>
                
            </th>
        </tr>

    </thead>
    <tbody>
        @foreach (var item in Model)
        {
        <tr class="icon-hand">
            <td>
                @Html.DisplayFor(i => item.Name)
            </td>
            <td>
                <a class="btn btn-warning mr-15" asp-action="Edit" asp-route-id="@item.Id"><i class="fa fa-edit fa-lg"></i></a>
                <a class="btn btn-info mr-15" asp-action="Details" asp-route-id="@item.Id"><i class="fa fa-info fa-lg"></i></a>
                <a class="btn btn-danger mr-15" asp-action="Delete" asp-route-id="@item.Id"><i class="fa fa-trash fa-lg"></i></a>
            </td>
        </tr>

        }
    </tbody>
</table>
