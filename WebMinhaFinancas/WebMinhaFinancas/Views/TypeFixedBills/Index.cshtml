@model IEnumerable<WebMinhaFinancas.Models.Entitty.TypeFixedBill>
@{
    ViewData["Title"] = "Tipo de Conta Fixa";
}
<h2 class="jumbotron">@ViewData["Title"]</h2>

<table class="table table-striped table-hover table-responsive">
    <thead>
        <tr>
            <th>
                Nome
            </th>
        </tr>

    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr class="icon-hand">
                <td>
                    @Html.DisplayFor(i => item.Name)
                </td>
            </tr>

        }
    </tbody>
</table>
